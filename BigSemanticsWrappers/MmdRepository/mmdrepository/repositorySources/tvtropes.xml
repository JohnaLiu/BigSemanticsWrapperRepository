<meta_metadata_repository name="tvtropes" package="ecologylab.bigsemantics.generated.library.tvtropes" default_user_agent_name="chrome_2">


<meta_metadata name="tvtrope_example" extends="compound_document" parser="xpath">
	
	<composite name="trope" type="tvtrope"/>
	<collection name="example_tropes" child_type="tvtrope"/>
	<collection name="comments" child_type="compound_document"/>
</meta_metadata>

<meta_metadata name="tvtrope_folder" extends="compound_document" parser="xpath">
	<collection name="examples" child_type="tvtrope_example"/>
	<collection name="ordered_examples" child_type="tvtrope_example"/>
</meta_metadata>
<meta_metadata name="tvtrope" extends="compound_document" parser="xpath">
	<selector url_path_tree="http://tvtropes.org/pmwiki/pmwiki.php/" domain="tvtropes.org"/>
	
	<scalar name="title" xpath="//div[@class='pagetitle']/span"/>
	

	<scalar name="quote" scalar_type="String" xpath="//div[@id='wikitext']/div[@class='indent']"/>
	<scalar name="quote_source" scalar_type="String" xpath="//div[@id='wikitext']/div[@class='indent']/div"/>
	
	<collection name="sub_tropes" child_type="tvtrope" xpath="//h2[text()='Tropes']/following-sibling::ul/li">
		<scalar name="title" xpath="./a/text()"/>
		<scalar name="location" xpath="./a/@href"/>
	</collection>
	
	<collection name="direct_descendants" child_type="tvtrope" xpath="//div[@id='wikitext']/ul/li">
		<scalar name="title" xpath=".//a[1]/text()"/>
		<scalar name="location" xpath=".//a[1]/@href"/>
	
	</collection>
	<collection name="examples_by_category" child_type="tvtrope_folder" xpath="//div[@class='folder']">
		<scalar name="title" xpath="./preceding-sibling::div[1]"/>
		<collection name="examples" xpath="./ul/li">
			<scalar name="title" xpath=".//a[1]"/>
			<scalar name="description" xpath="."/>
			<collection name="example_tropes" xpath="//a[@class='twikilink']">
				<scalar name="title" xpath="./text()"/>
				<scalar name="location" xpath="./@href"/>
			</collection>
			<collection name="comments" child_type="compound_document" xpath="./ul/li">
				
				<scalar name="description" xpath="."/>
			</collection>
		</collection>
		<collection name="ordered_examples" xpath="./ol/li">
			<scalar name="title" xpath=".//a[1]"/>
			<scalar name="description" xpath="."/>
			<composite name="trope" xpath=".//a[1]">
				<scalar name="title" xpath="./text()"/>
				<scalar name="location" xpath="./@href"/>
			</composite>
			<collection name="comments" child_type="compound_document" xpath="./ul/li">
				
				<scalar name="description" xpath="."/>
			</collection>
		</collection>
	</collection>
	<collection name="all_linked_tropes" child_type="tvtrope" xpath="//a[@class='twikilink']">
		<scalar name="title" xpath="./text()"/>
		<scalar name="location" xpath="./@href"/>
	</collection>
</meta_metadata>


</meta_metadata_repository>