	<meta_metadata name="asos_page" extends="compound_document" parser="xpath" user_agent_name="chrome" >
		<selector url_regex="http://us.asos.com/[A-Za-z-]*/[A-Za-z0-9]*/.*" domain="asos.com" >
  			<param name="cid" />
		</selector>
		<example_url url="http://us.asos.com/Women-Dresses-Summer-Dresses/vxlwy/?cid=10860" />
		
		<scalar name="title" xpath="//span[@id='ctl00_ContentMainPage_ctlBreadCrumbs_lblBreadCrumbs']/span[3]/following-sibling::text()" />
		<scalar name="description" xpath="//p[@class='copy']" />
		
		<collection name="products" child_type="asos" xpath="//ul[@id='items']/li/a" >
			<scalar name="title" xpath="." />
			<scalar name="location" xpath="./@href" />
		</collection>
	</meta_metadata>
	 
	 
	<meta_metadata name="asos" extends="product" parser="xpath" user_agent_name="chrome" >
		<selector url_regex="http://us.asos.com/[ASOS/]*[A-Za-z0-9-]*/[0-9Pa-z]*/.*" domain="asos.com" >
		 	<param name="iid" />
		</selector>
		<example_url url="http://us.asos.com/Warehouse-Floral-Skater-Dress/12p10b/?iid=3735865&amp;cid=15801&amp;sh=0&amp;pge=0&amp;pgesize=36&amp;sort=-1&amp;clr=Multi&amp;mporgp=L1dhcmVob3VzZS9XYXJlaG91c2UtRmxvcmFsLVNrYXRlci1EcmVzcy9Qcm9kLw.." />
	
		<scalar name="title" xpath="//span[@class='product_title']" />
		<scalar name="price" xpath="//span[@class='product_price_details']" />
		<composite name="image" xpath="//img[@class='main-image xl']" >
			<scalar name="location" xpath="./@src" />
		</composite>
		
		<collection name="details" child_type="document" xpath="//div[@class='product-description']/ul/li/ul/li" >
			<scalar name="title" label="" xpath="." />
		</collection>
		<!-- can make not expandable???? -->
		
		<composite name="designer" extends="asos_page" xpath="//div[@class='product-description']//li[@class='single-entry']" >
			<scalar name="title" xpath="./a[2]/strong" />
			<scalar name="location" xpath="./a[2]/@href" />
			<scalar name="description" xpath="//div[@id='separates_till_box']/div/ul/li[@class='single-entry']" />
		</composite>
		
		<collection name="recommmended_products" child_type="asos" xpath="/div[@class='carousel-wrapper']//div[@class='item']/a[1]" >
			<scalar name="title" xpath="./@title" />
			<scalar name="location" xpath="./@href" />
		</collection>
		
		<collection name="tags" child_type="compound_document" xpath="//li[contains (@id, 'divFindSimilar')]/a[3]" >
			<scalar name="title" xpath="." />
			<scalar name="location" xpath="./@href" />
		</collection>
		
	</meta_metadata>
	
	
