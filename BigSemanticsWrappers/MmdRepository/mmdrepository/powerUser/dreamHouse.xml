  		<scalar name="title" xpath=".//span[@class='item']" />
  		<scalar name="price" xpath=".//span[@class='currency currency']" />
  	</collection>
     	
  	<composite name="image">
  		<scalar name="location" xpath="//img[@id='hero']/@src"/>
  	</composite>

  </meta_metadata> 

