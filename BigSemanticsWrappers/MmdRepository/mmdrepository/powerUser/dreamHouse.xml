<meta_metadata_repository name="dream_house"
                          package="ecologylab.bigsemantics.generated.library.dreamHouse"
                          default_user_agent_name="firefox_3_6_8">

  <meta_metadata name="glass_tile_store_product" type="product" parser="xpath">
  	<selector url_regex="http://www.glasstilestore.com/\w+-p/\w+.htm" domain="glasstilestore.com" />
  	
  	<scalar name="title" xpath="//font[@class='productnamecolorLARGE colors_productname']/text()" />
  	<scalar name="description" xpath="//table[@class='colors_descriptionbox'//p/text()"/>
  	<scalar name="price" xpath="//font[@class='pricecolor']/text()" />
  </meta_metadata>
  
  <meta_metadata name="urban_hardwoods_product" type="product" parser="xpath">
  	<selector url_stripped="http://www.urbanhardwoods.com/furniture/product.aspx"/>
  	
  	<scalar name="title" xpath="//span[@class='productName']/text()"/>
  	<scalar name="description" xpath="//span[@class='furnitureINTRO']/div[0]/text()"/>
  </meta_metadata>
  
  <meta_metadata name="home_depot_product" extends="product" parser="xpath" user_agent_name="chrome">
  	<selector url_regex_fragment="http://www.homedepot.com/p/[^/]*" domain="homedepot.com" />
  	
  	<example_url location="http://www.homedepot.com/p/Home-Decorators-Collection-70-5-in-H-x-69-75-in-W-Cherry-Wood-and-Bamboo-4-Panel-Room-Divider-5852120120/202368416?N=bdtw#.UY56OVf_E4k" />
  	
  	<scalar name="title" xpath="//span[@itemprop='name']"/>
  	<scalar name="description" xpath="//span[@itemprop='description']"/>
  	<scalar name="price" xpath="//span[@itemprop='price']" >
  		<filter regex="/[a-z|A-Z|-]*" replace="" />
  	</scalar>
  	<scalar name="overall_rating" xpath="//div[@id='BVRRRatingOverall_']/div[3]/span[1]"/>
  	
  	<collection name="reviews" xpath="//div[@id='BVRRDisplayContentBodyID']//div[@class='BVRRReviewDisplayStyle5']">
		<scalar name="rating" xpath=".//div[@class='BVRRRatingNormalOutOf'][1]" />
		<scalar name="description" xpath="./span[@class='BVRRReviewText']" />
	</collection>
  	
  	<collection name="people_also_buy" child_type="home_depot_product" xpath="//div[@id='ymal_vert']/div[@class='product22']" >
  		<scalar name="location" xpath=".//div[@class='product-name']/a/@href" />
  		<scalar name="title" xpath=".//div[@class='product-name']/" />
  		<scalar name="price" xpath=".//div[@class='price']" />
  	</collection>
     	
  	<composite name="image">
  		<scalar name="location" xpath="//img[@itemprop='image']/@src"/>
  	</composite>
  	
  </meta_metadata>
 
  
  <meta_metadata name="remodelista_product" type="product" parser="xpath">
  	<selector url_path_tree="http://remodelista.com/products/"/>
  	
  	<scalar name="title" xpath="//h2[@class='cinza']/text()"/>
  	<scalar name="description" xpath="//span[@class='mce-content']/p[2]/font/text()" />
  	<scalar name="price" xpath="//span[@class='cinza bigfont']/text()" >
  		<filter regex="Price: " replace="" />
  	</scalar>
  </meta_metadata>
  
  <meta_metadata name="overstock_com_product" extends="product" parser="xpath">
  	<selector url_regex="http://www.overstock.com/[^/]+/[^/]+/[0-9]+/product.html.*"
              domain="overstock.com" />
  	
  	<example_url url="http://www.overstock.com/Home-Garden/Bodipedic-Essentials-8-inch-Queen-size-Memory-Foam-Mattress/6153386/product.html?rcmndsrc=4" />
  	
  	<scalar name="title" xpath="//div[@itemprop='name']/h1"/>
  	<!--
  	<scalar name="description" xpath="//div[@id='prodMain_descShort']//li[1]/text()"/>
  	-->
  	<scalar name="description" xpath="//ul[@id='details_descFull']/text()"/>
  	<scalar name="price" xpath="//span[@itemprop='price']/text()"/>
  	

  
    <scalar name="overall_rating" xpath="//div[@itemprop='ratingValue']">
<!--       <filter regex="Rating " replace="" /> -->
    </scalar>
    
    <collection name="reviews" xpath="//div[@id='most-helpful-reviews']/div[2]/ul/li">
      <scalar name="rating" xpath=".//p[@class='helpful-review-rating']/i/@title" />
      <scalar name="title" xpath=".//h4[@class='review-title']" />
      <scalar name="description" xpath="./div[@class='review-copy']/p" />
    </collection>
    
    <composite name="image">
  		<scalar name="location" xpath="//div[@id='proImageHero' or @id='galleryImage']//img/@src"/>
  	</composite>
    
  </meta_metadata>
  
  <meta_metadata name="ikea_product" extends="product" parser="xpath" user_agent_name="chrome">
  	<selector url_regex_fragment="http://www.ikea.com/us/en/catalog/products/[^/]*" domain="ikea.com" />
  	
  	<example_url location="http://www.ikea.com/us/en/catalog/products/60204805/" />
  	
  	<scalar name="title" xpath="//h1"/>
  	<scalar name="description" xpath="//div[@class='salesArguments']"/>
  	<scalar name="price" xpath="//div[@id='prodPrice']//span[@class='packagePrice']" />
  	  	
  	<composite name="image">
  		<scalar name="location" xpath="//img[@id='productImg']/@src"/>
  	</composite>

  </meta_metadata>
  
  
  <meta_metadata name="lowes_product" extends="product" parser="xpath" user_agent_name="firefox">
  	<selector url_regex_fragment="http://www.lowes.com/*" domain="lowes.com" />
  	
  	<example_url location="http://www.lowes.com/pd_331470-817-235159_4294703024%2B4294934373" />
  	
  	<scalar name="title" xpath="//div[@class='itemInfo']/h1"/>
  	<scalar name="description" xpath="//div[@id='description-tab']/p"/>
  	<scalar name="price" xpath="//div[@id='pricing']/span[@class='price']" />

  	<collection name="related_items" child_type="lowes_product" xpath="//div[@id='relatedItems']/div" >
  		<scalar name="location" xpath="./a[2]/@href" />
  		<scalar name="title" xpath="./a[2]" />
  		<scalar name="price" xpath="./p/span" />
  	</collection>
   	
<!--   	<scalar name="model" xpath="//input[@name='modelNumber']/@value"/> -->
  	
  	<composite name="image">
  		<scalar name="location" xpath="//div[@id='imgCont']//img[@class='productimage']/@src"/>
  	</composite>
  	
  	<semantic_actions>
 		<get_field name="image" />
		<if>
			<not_null value="image" />
		    <create_and_visualize_img_surrogate>
		    	<arg name="metadata" value="image" />
		    </create_and_visualize_img_surrogate>
		</if>
	</semantic_actions>
  </meta_metadata> 
  
    <meta_metadata name="crateandbarrel_product" extends="product" parser="xpath" user_agent_name="firefox">
  	<selector url_regex_fragment="http://www.crateandbarrel.com/[^/]*/[^/]*" domain="crateandbarrel.com" />
  	
  	<example_url location="http://www.crateandbarrel.com/petrie-leather-chair/s165967" />
  	
  	<scalar name="title" xpath="//h1[@itemprop='name']"/>
  	<scalar name="description" xpath="//div[@itemprop='description']/p[1]"/>
  	<scalar name="price" xpath="//span[@itemprop='price']" />

  	<collection name="reviews" xpath="//div[@class='BVRRDisplayContentBody']/div">
		<scalar name="title" xpath=".//div[@class='BVRRReviewTitleContainer']/span[2]" />
		<scalar name="rating" xpath=".//div[@class='BVRRRatingNormalImage']/img/@title" />
		<scalar name="description" xpath=".//div[@class='BVRRReviewTextContainer']//span[2]" />
	</collection>

  	<collection name="customer_also_viewed" child_type="crateandbarrel_product" xpath="//ul[@id='carouselRecommended']/li" >
  		<scalar name="location" xpath=".//a[@class='productNameLink']/@href" />
  		<scalar name="title" xpath=".//span[@class='productName']" />
  		<scalar name="price" xpath=".//span[@class='salePrice' or @class='regPrice']" />
  	</collection>
   	  	
  	<composite name="image">
  		<scalar name="location" xpath="//img[@itemprop='image']/@src"/>
  	</composite>
  	
  </meta_metadata> 
  
  <meta_metadata name="worldmarket_product" extends="product" parser="xpath" user_agent_name="firefox">
